{
  "name": "package-majors",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for package-majors goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:production": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "pnpm test:ember",
    "test:ember": "NODE_ENV=development vite build --mode development && testem ci --port 0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/compat": "^4.1.12",
    "@embroider/core": "^4.4.2",
    "@embroider/vite": "^1.5.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.1.0",
    "@glint/template": "^1.7.4",
    "@glint/tsserver-plugin": "^2.1.0",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/chart.js": "^4.0.1",
    "@types/color": "^4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "@types/semver": "^7.5.8",
    "concurrently": "^9.1.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.10.0",
    "ember-template-lint": "^7.7.0",
    "eslint": "^9.27.0",
    "esyes": "^1.0.3",
    "fs-extra": "^11.3.0",
    "loader.js": "^4.7.0",
    "node-confirm": "^1.0.1",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.4.0",
    "terser": "^5.46.0",
    "testem": "^3.16.0",
    "typescript": "~5.9.3",
    "validate-npm-package-name": "^7.0.1",
    "vite": "7.3.1",
    "yesno": "^0.4.0"
  },
  "packageManager": "pnpm@10.26.2",
  "pnpm": {
    "overrides": {
      "ember-primitives": "^0.53.0",
      "@glimmer/component": "^2.0.0",
      "@embroider/macros": "1.19.6"
    }
  },
  "engines": {
    "node": ">= 20.x"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "imports": {
    "#app/*": "./app/*",
    "#services/*": "./app/services/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "dependencies": {
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "1.19.6",
    "chart.js": "^4.4.9",
    "chartjs-plugin-annotation": "^3.1.0",
    "color": "^5.0.3",
    "decorator-transforms": "^2.3.0",
    "ember-modifier": "^4.2.2",
    "ember-primitives": "^0.53.0",
    "ember-resources": "^7.0.4",
    "nvp.ui": "^0.5.3",
    "reactiveweb": "^1.4.2",
    "semver": "^7.7.2"
  }
}
